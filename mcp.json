      "command": "npx",
      "args": ["-y", "github:aaronsb/think-strategies"]
    },
    "creative-thinking": {
      "command": "node",
      "args": ["/Users/ww/dev/tools/mcp/creative-thinking/dist/index.js"]
    },
    "ableton": {
      "command": "uvx",
      "args": ["ableton-mcp"]
