    },
    "fetch": {
      "command": "npx",
      "args": ["mcp-fetch-server"]
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp", "--api-key", "${CONTEXT7_API_KEY}"]
