{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "atom-of-thoughts": {
      "command": "node",
      "args": ["/Users/ww/dev/tools/mcp/MCP_Atom_of_Thoughts/build/index.js"]
    },
